            $(".boarder").removeAttr('disabled');
        }
    }
</script>
<script>
    function dropAbility() {
        var member = $("#ability").val();
        if (member == 1) {
            $("#showme_button").attr('disabled', 'disabled');
            $("#reason").attr("readonly", true);
            $(".de").attr("readonly", true);
            $("#registration").attr("readonly", true);
            $("#registration_dates").attr('disabled', true);
        }
        else if ((member == 2 || member == 3)) {
            $("#showme_button").removeAttr("disabled");
            $("#reason").removeAttr("readonly", true);
            $(".de").removeAttr("readonly", true);
            $("#registration").removeAttr("readonly", true);
            $("#registration_dates").removeAttr('disabled');
        }
    }
</script>

