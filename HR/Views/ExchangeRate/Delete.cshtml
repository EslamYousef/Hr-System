@model HR.Models.Exchange_Rate
@using HR.Resource;




@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/MainLayOut.cshtml";
}

<h3>@Basic.message_delete</h3>
@using (Html.BeginForm("Delete", "ExchangeRate", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">

        <hr />

        @if (TempData["Message"] != null)
        {
            <div class="alert alert-danger alert-dismissible" role="alert">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                @TempData["Message"]
            </div>
        }
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value=@Basic.Delete class="btn btn-success" />
                <a href="@Url.Action("index","ExchangeRate")" class="btn btn-danger btn-sm"><i class="fa fa-trash-o"></i>@Basic.Back</a>

            </div>
        </div>
    </div>
}

@section scripts{}